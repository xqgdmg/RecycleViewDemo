<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.md.demo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MDMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".recyclerview.MDRvMainActivity"/>
        <activity android:name=".recyclerview.MDLinearRvActivity"/>
        <activity android:name=".recyclerview.MDGridRvActivity"/>
        <activity android:name=".recyclerview.MDStaggeredRvActivity"/>
        <activity android:name=".recyclerview.MDSwipeToDismissActivity"/>
        <activity android:name=".recyclerview.MDDragRvActivity"/>
    </application>

</manifest>